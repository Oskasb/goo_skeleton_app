[{

    "template":{
        "top_bar":{
            "layers": [
                {
                    "shape":"top_bar_box",
                    "layers": [
                        {
                            "shape":"stats_tracker",
							"on_apply":[{"call":"gui_toggle_template", "params":{"template":"messages"}}],
                            "text":{
                                "callback":"frame_tpf",
                                "on_hover":
                                {
                                    "callback":"more_stats"
                                },
								"on_active":
								{
									"callback":"more_stats"
								}
                            }
                        },
                        {
                            "shape":"tpf_graph",
							"text":{
								"key":"tfp_graph",
							"on_active":
							{
								"key":"frame_tpf",
								"callback":"more_stats"
							}
							}
                        }
                    ]
                }
            ]
        }
    }
}]